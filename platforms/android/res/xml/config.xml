<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.show0017.midterm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="LocationAndSettings">
        <param name="android-package" value="sk.tamex.cordova.plugins.LocationAndSettings" />
    </feature>
    <icon density="mdpi" src="www/img/ic_launcher_mdpi.png" />
    <icon density="hdpi" src="www/img/ic_launcher_hdpi.png" />
    <icon density="xhdpi" src="www/img/ic_launcher_xhdpi.png" />
    <icon density="xxhdpi" src="www/img/ic_launcher_xxhdpi.png" />
    <icon density="xxxhdpi" src="www/img/ic_launcher_xxhdpi.png" />
    <name>Show0017_Midterm</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="show0017@algonquinlive.com" href="https://github.com/show0017/midterm-app">
        Wael Showair Midterm App
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
